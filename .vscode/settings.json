{
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.fixAll.eslint": "explicit",
		"eslint.autoFixOnSave": "explicit",
		"source.organizeImports": "never",
		"source.fixAll.stylelint": "explicit"
	},
	"eslint.validate": [
		"typescript",
		"javascript"
	],
	"cSpell.words": [
		"defaultify",
		"femboy",
		"Femboys"
	]
}